{
  "name": "trials-of-faith",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "vite build --base=/ && surge ./dist http://trials-of-faith.surge.sh",
    "build-github-master": "vite build --base=/trials-of-faith/ --outDir=dist",
    "build-github-develop": "vite build --base=/trials-of-faith/develop/ --outDir=dist/develop"
  },
  "git": {
    "scripts": {
      "pre-push": "env-cmd -f ./.env cross-env-shell \"surge ./dist $REMOTE_URL\""
    }
  },
  "devDependencies": {
    "@yushijinhun/three-minifier-rollup": "^0.4.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "git-scripts": "^0.6.4",
    "path": "^0.12.7",
    "rollup-plugin-images": "^1.0.0",
    "sass": "^1.54.6",
    "vite": "^3.0.7"
  },
  "dependencies": {
    "@enable3d/ammo-physics": "^0.25.2",
    "@enable3d/three-graphics": "^0.25.2",
    "autoprefixer": "^10.4.8",
    "colyseus.js": "^0.14.13",
    "cors": "^2.8.5",
    "daisyui": "^2.6.0",
    "dotenv": "^16.0.2",
    "enable3d": "^0.25.2",
    "gsap": "^3.10.4",
    "http": "^0.0.1-security",
    "lil-gui": "^0.17.0",
    "postcss": "^8.4.7",
    "stats.js": "^0.17.0",
    "tailwindcss": "^3.0.23",
